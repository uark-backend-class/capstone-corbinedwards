<h1>{{username}}</h1>
<br />
<form method="POST">
    <input type="submit" name="logout" value="Log Out" />
</form>
<br />
<h2><a href="/app">Find Recipes</a></h2>
<br />
<h2>Saved Recipes</h2>
<ul id="saved-recipes">
    {{#each savedRecipes}}
        <li>{{this.name}}</li>
        {{else}}
        <li>No saved recipes.</li>
    {{/each}}
</ul>

<script src="/scripts/profile.js"></script>
